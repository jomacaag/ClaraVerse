  "customNodes": [
    {
      "id": "node-1760370465408-zc656cd66",
      "type": "weather-astro-api-request",
      "name": "Weather Astro API Request",
      "description": "Fetches astronomical weather data (sunrise, sunset, moon phase, etc.) using longitude and latitude coordinates via the 7Timer API. Returns structured JSON response.",
      "category": "custom",
      "icon": "üåê",
      "inputs": [
        {
          "id": "input_1760370444596_8azodxx4h",
          "name": "longitude",
          "type": "input",
          "dataType": "number",
          "required": true,
          "description": "The longitude coordinate (e.g., 113.2)"
        },
        {
          "id": "input_1760370444596_qf2h4qahh",
          "name": "latitude",
          "type": "input",
          "dataType": "number",
          "required": true,
          "description": "The latitude coordinate (e.g., 23.1)"
        }
      ],
      "outputs": [
        {
          "id": "output_1760370444596_hoiutmflp",
          "name": "response",
          "type": "output",
          "dataType": "object",
          "description": "The full JSON response from the 7Timer API"
        },
        {
          "id": "output_1760370444596_fjmdqw0oo",
          "name": "sunrise",
          "type": "output",
          "dataType": "string",
          "description": "Sunrise time in HH:MM format"
        },
        {
          "id": "output_1760370444596_h442vg4qg",
          "name": "sunset",
          "type": "output",
          "dataType": "string",
          "description": "Sunset time in HH:MM format"
        },
        {
          "id": "output_1760370444596_nf6xggkpp",
          "name": "moon_phase",
          "type": "output",
          "dataType": "string",
          "description": "Current moon phase description"
        }
      ],
      "properties": [
        {
          "id": "prop_1760370444596_58jl71jm2",
          "name": "unit",
          "type": "string",
          "required": false,
          "defaultValue": "metric",
          "description": "Temperature unit (metric, imperial)"
        },
        {
          "id": "prop_1760370444596_ty9qluhii",
          "name": "timezone_shift",
          "type": "number",
          "required": false,
          "defaultValue": "0",
          "description": "Adjust for local timezone offset (in hours)"
        }
      ],
      "executionCode": "async function execute(inputs, properties, context) {\n  const { longitude, latitude } = inputs;\n  const { unit, timezone_shift } = properties;\n\n  const url = `https://www.7timer.info/bin/astro.php?lon=${longitude}&lat=${latitude}&ac=0&unit=${unit}&output=json&tzshift=${timezone_shift}`;\n\n  try {\n    const response = await fetch(url);\n    if (!response.ok) {\n      throw new Error(`HTTP error! status: ${response.status}`);\n    }\n    const data = await response.json();\n\n    // Extract key fields\n    const sunrise = data.dataseries?.[0]?.sunrise || 'N/A';\n    const sunset = data.dataseries?.[0]?.sunset || 'N/A';\n    const moon_phase = data.dataseries?.[0]?.moonphase || 'N/A';\n\n    context.log('API Request Successful:', url);\n\n    return {\n      response: data,\n      sunrise: sunrise,\n      sunset: sunset,\n      moon_phase: moon_phase\n    };\n  } catch (error) {\n    context.log('Error fetching weather data:', error.message);\n    throw error;\n  }\n}",
      "metadata": {
        "tags": [
          "weather",
          "api",
          "astronomy",
          "geolocation",
          "7timer",
          "coordinates"
        ],
        "documentation": "",
        "examples": []
      }
    }
